{
    "name": "heybigname/event-dispatcher",
    "version": "1.1.0",
    "description": "Event Dispatcher with a focus on Domain Events",
    "license": "MIT",
    "authors": [
        {
            "name": "Mitchell van Wijngaarden",
            "email": "mitchell@kooding.nl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/container": "~4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "BigName\\EventDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BigName\\EventDispatcher\\Stubs\\": "spec/stubs/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
